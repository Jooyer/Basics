<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--自定义圆角矩形-->
    <declare-styleable name="RoundImageView">
        <attr name="RoundImageView_maskType">
            <enum name="RECTANGLE" value="0" />
            <enum name="CIRCLE" value="1" />
            <enum name="ROUNDRECTANGLE" value="2" />
            <enum name="ROUNDRECTANGLETOP" value="3" />
        </attr>
        <attr name="RoundImageView_roundRadius" format="dimension" />
        <attr name="RoundImageView_borderWidth_Ri" format="dimension" />
        <attr name="RoundImageView_borderColor_Ri" format="color|reference" />
    </declare-styleable>

</resources>