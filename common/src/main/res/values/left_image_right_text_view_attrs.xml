<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LeftImgAndRightTextView">
        <attr name="lirt_backColor" format="color"/>
        <attr name="lirt_backColorPress" format="color"/>
        <attr name="lirt_textColor" format="color"/>
        <attr name="lirt_textColorPress" format="color"/>
        <attr name="lirt_iconDrawable" format="reference" />
        <attr name="lirt_iconDrawablePress" format="reference" />
        <attr name="lirt_text" format="string" />
        <attr name="lirt_textSize" format="float" />
        <attr name="lirt_spacing" format="dimension" />
        <attr name="lirt_style">
            <enum name="iconLeft" value="0" />
            <enum name="iconRight" value="1" />
            <enum name="iconUp" value="2" />
            <enum name="iconBottom" value="3" />
        </attr>
    </declare-styleable>

</resources>