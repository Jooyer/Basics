<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="VoiceAnimView">
        <!-- 弧形线条数量,默认3条 -->
        <attr name="vav_arc_count" format="integer" />
        <!-- 正常颜色, 默认值 #c9c9c9 -->
        <attr name="vav_normal_color" format="color" />
        <!-- 选中颜色, 默认值 #4b4b4b -->
        <attr name="vav_checked_color" format="color" />
        <!-- 动画执行时间,默认 2000毫秒-->
        <attr name="vav_anim_duration" format="integer" />

    </declare-styleable>

</resources>